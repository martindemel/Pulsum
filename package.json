{
  "name": "pulsum-wellness-app",
  "version": "1.0.0",
  "description": "A comprehensive wellness dashboard integrating Oura and Dexcom data with AI-powered recommendations",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon backend/server.js",
    "client": "cd frontend && npm start",
    "init-db": "node backend/scripts/init-db.js",
    "install-all": "npm install && cd frontend && npm install",
    "build": "cd frontend && npm run build"
  },
  "keywords": ["wellness", "health", "oura", "dexcom", "ai"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "concurrently": "^8.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.2",
    "nodemon": "^3.0.1",
    "openai": "^4.20.0",
    "sqlite": "^5.0.1",
    "sqlite3": "^5.1.6"
  }
} 